[run]
source = dev
omit =
    */tests/*
    */conftest.py
    */__pycache__/*
    */migrations/*
    */fix_migration.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
